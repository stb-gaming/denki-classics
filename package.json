{
	"name": "denki-classics",
	"version": "0.0.1",
	"description": "Beehive Bedlam, just Beehive Bedlam",
	"main": "index.js",
	"scripts": {
		"start": "electron .",
		"watchSass": "sass --watch menu/scss:menu/css",
		"buildSass": "sass menu/scss:menu/css",
		"buildWindows": "electron-builder build --win portable zip --x64 --ia32 --publish never",
		"buildLinux": "electron-builder build --linux AppImage tar.gz --publish never",
		"buildMac": "electron-builder build --mac pkg --publish never"
	},
	"repository": "git+https://github.com/stb-gaming/denki-classics.git",
	"author": "tumble1999",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/stb-gaming/denki-classics/issues"
	},
	"homepage": "https://github.com/stb-gaming/denki-classics#readme",
	"devDependencies": {
		"electron": "^25.2.0",
		"electron-builder": "^24.4.0",
		"sass": "^1.63.6"
	},
	"build": {
		"appId": "io.github.stb-gaming.denki-classics",
		"mac": {
			"category": "your.app.category.type"
		},
		"win": {
			"icon": "assets/img/bee.ico"
		}
	}
}
